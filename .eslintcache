ps);\n    this.state = {\n      todos: [\n        { id: 1, msg: \"finish the application\", done: false },\n        { id: 2, msg: \"todo application\", done: true },\n        { id: 3, msg: \"integrate .net project for backend\", done: true },\n      ]\n    }\n    this.delete.bind(this)\n    this.mark.bind(this)\n  }\n\n  addEntry = (msg) => {\n    const ids = this.state.todos.map((item) => {\n      return item.id\n    });\n    let id = Math.max(...ids) + 1;\n    if (id<=0){\n      id=1;\n    }\n\n    this.setState(prevState => {\n      return { todos: [...prevState.todos, { id: id, msg: msg, done: false }] }\n    })\n  }\n\n  mark = (id) => {\n    let allTodos = this.state.todos.map(p => {\n      if (p.id === id) {\n        p.done = !p.done;\n      }\n      return p;\n    });\n\n\n    this.setState({ todos: allTodos })\n  }\n\n  delete = (id) => {\n    // geht irgendwie nicht\n    this.setState({todos: this.state.todos.filter(p=>  p.id !== id)})\n\n    console.log(this.state.todos)\n  }\n\n  render() {\n    return (\n      <div className=\"App\">\n        <Header />\n        <div className=\"container\">\n          <div className=\"row mb-5\">\n            <NewTodo add={this.addEntry} />\n          </div>\n          <div className=\"row\">\n          <TodoList todos={this.state.todos} mark={this.mark} deleteHandler={this.delete} />\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default App;\n","D:\\repos\\react\\todoapp\\src\\components\\TodoList.js",[],"D:\\repos\\react\\todoapp\\src\\components\\Header.js",[],"D:\\repos\\react\\todoapp\\src\\components\\Todo.js",[],"D:\\repos\\react\\todoapp\\src\\components\\NewTodo.js",[],{"ruleId":"31","replacedBy":"32"},{"ruleId":"33","replacedBy":"34"},{"ruleId":"35","severity":1,"message":"36","line":1,"column":10,"nodeType":"37","messageId":"38","endLine":1,"endColumn":16},"no-native-reassign",["39"],"no-negated-in-lhs",["40"],"no-unused-vars","'render' is defined but never used.","Identifier","un